#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules/touch/bin/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules/touch/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/node_modules:/mnt/d/EveryDay/Out or In/原生实现/node_modules:/mnt/d/EveryDay/Out or In/node_modules:/mnt/d/EveryDay/node_modules:/mnt/d/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules/touch/bin/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules/touch/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/node_modules:/mnt/d/EveryDay/Out or In/原生实现/node_modules:/mnt/d/EveryDay/Out or In/node_modules:/mnt/d/EveryDay/node_modules:/mnt/d/node_modules:/mnt/d/EveryDay/Out or In/原生实现/前端部署/1.简单部署/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
